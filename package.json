{
  "name": "rsscloud-server",
  "version": "1.1.1",
  "description": "An rssCloud Server",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --use_strict ./app.js",
    "client": "nodemon --use_strict ./client.js",
    "import-data": "node ./bin/import-data.js",
    "jshint": "echo '=> linting' && jshint ./**/*.js",
    "eslint": "echo '=> sniffing' && eslint controllers/ services/",
    "test": "mocha --timeout 10000 --bail",
    "test-api": "docker-compose up --build --abort-on-container-exit"
  },
  "engines": {
    "node": ">=10.6.0"
  },
  "author": "Andrew Shell <andrew@andrewshell.org>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "console-stamp": "^0.2.9",
    "cors": "^2.8.5",
    "davexmlrpc": "^0.4.26",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-ws": "^4.0.0",
    "markdown-it": "^10.0.0",
    "moment": "^2.27.0",
    "mongodb": "3.5.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "request": "^2.88.0",
    "request-promise-native": "1.0.8",
    "sprintf-js": "^1.1.2",
    "xml2js": "^0.4.23",
    "xmlbuilder": "^13.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewshell/rsscloud-server.git"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-json": "^1.0.0",
    "chai-xml": "^0.3.2",
    "eslint": "^6.6.0",
    "eslint-config-crockford": "^2.0.0",
    "jshint": "^2.12.0",
    "mocha": "^7.2.0",
    "nodemon": "2.0.2",
    "supertest": "^4.0.2"
  }
}
