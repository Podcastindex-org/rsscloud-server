{
  "name": "rsscloud-server",
  "version": "1.1.1",
  "description": "An rssCloud Server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --use_strict ./app.js",
    "client": "nodemon --use_strict ./client.js",
    "import-data": "node ./bin/import-data.js",
    "jshint": "echo '=> linting' && jshint ./**/*.js",
    "eslint": "echo '=> sniffing' && eslint controllers/ services/",
    "pretest": "npm run jshint -s && npm run eslint -s",
    "test": "echo '=> testing' && nyc mocha -- --recursive -R spec -r should",
    "posttest": "nyc check-coverage"
  },
  "engines": {
    "node": ">=10.6.0"
  },
  "author": "Andrew Shell <andrew@andrewshell.org>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "console-stamp": "^0.2.9",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-ws": "^4.0.0",
    "moment": "^2.24.0",
    "mongodb": "3.5.0",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "request": "^2.88.0",
    "request-promise-native": "1.0.8",
    "sprintf-js": "^1.1.2",
    "xmlbuilder": "^13.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewshell/rsscloud-server.git"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-crockford": "^2.0.0",
    "jshint": "^2.10.3",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nodemon": "2.0.2",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "sinon": "^7.5.0"
  }
}
